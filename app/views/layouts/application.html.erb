<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>ScopoApp</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <!-- link rel="stylesheet" type="text/css" href="app/assets/stylesheets/styles2.css" --> 
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "styles2", "data-turbo-track": "reload" %>
  </head>
  <body>

    <%= render 'home/header' %>
    <div class="container"> 

      <br/>

      <% if notice %>
        <%= render 'layouts/alerts' %>
      <% end %>

        <%= yield %>
    </div>
    <%= render 'home/footer' %>

    <!-- %= javascript_include_tag "jquery-3.1.1.min", "data-turbolinks-track" => true  %-->
    <%= javascript_include_tag "ajax-utils", "data-turbolinks-track" => true  %>
    <%= javascript_include_tag "script", "data-turbolinks-track" => true  %>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity=">sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
    <!-- script src="app/assets/javascript/jquery-3.1.1.min.js"></script -->
    <!-- script src="app/assets/javascript/ajax-utils.js"></script -->
    <!-- script src="app/assets/javascript/script.js"></script --> 
  </body>
</html>
